<body>
      <!-- Navbar and sidebar start -->

      <input type="checkbox" id = "check">
      <nav id= "myNav" class="navbar fixed-top">
          <ul class="navbar-nav">
              <label for="check">
                  <i class="fas fa-bars" id="sidebar_btn"></i>
              </label>
          </ul>
          <a id="myBrand" class="navbar-brand" href="/home/<%= currentUser._id %>">Tango</a>
      </nav>
      
      <div class="sidebar">
          <center class="mr-5">
              <img src="https://images.unsplash.com/photo-1476041800959-2f6bb412c8ce?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=500&q=60" class="profile_image" alt="">
              <h4>Parth</h4>
              <form id="bottom" action="/explore" method="GET" class="form-inline">
                  <input id ="myInput" name="fullName" class="form-control" type="search" placeholder="Search By Name" aria-label="Search">
                  <button type="submit" class="btn btn-outline-primary rounded-0 mb-4">Search</button>
              </form>
          </center>
          <div class="scrollable">
              <a href="/profile/<%= currentUser._id %>"><i class="fas fa-user"></i><span>Profile</span></a>
              <a href="/explore"><i class="fas fa-search"></i><span>Explore</span></a>
              <a href="#"><i class="fas fa-heart"></i><span>Likes</span></a>
              <a href="/savedProfiles/<%= currentUser._id %>" ><i class="fas fa-bookmark"></i><span>Saved</span></a>
              <a href="#"><i class="fas fa-comments"></i><span>Chat</span></a>
              <a href="#"><i class="fas fa-cog"></i><span>Settings</span></a>
              <a href="/logout"><i class="fas fa-sign-out-alt"></i><span>Logout</span></a>
          </div>
      </div>
  
      <!-- Navbar and sidebar end -->

