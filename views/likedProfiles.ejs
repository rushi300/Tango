    <div class="content">
        <div class="container" style="padding-top: 150px;">
            <ul class="nav justify-content-center nav-tabs" id="myTab" role="tablist">
                <li class="nav-item" role="presentation">
                    <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home"
                        aria-selected="true">Liked By</a>
                </li>
                <li class="nav-item" role="presentation">
                    <a class="nav-link"  id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile"
                        aria-selected="false">Liked</a>
                </li>
            </ul>
            <div class="tab-content" id="myTabContent">
                <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
                    <!-- Liked By content -->
                    <section id="team" >
                        <div class="container my-3 text-center">
                            <div class="row mb-5">
                                <div class="col mt-3 my-5">
                                    <h4 class="mb-5 pb-5 display-5"> Users that have Liked Your Profile</h4>
                                </div>
                            </div>
                            
                            <div class="row mt-2">
                                <% arr.forEach((User)=> { %>
                                    <div class="col-lg-4 col-md-6">
                                        <a href="/profile/<%= User._id %>">
                                            <div class="card">
                                                <div class="card-body">
                                                    <img src="<%= User.image %>"
                                                        alt="" class="img-fluid rounded-circle mb-3" >
                                                    <h3 class="card-title"><%= User.fullName %> </h3>
                                                    <h5 class="py-1 "><%= User.age %> &bull;  <%= User.username %> &bull;  <%= User.city %></h5>
                                                    <p>
                                                        <% if (User.bio.length <= 150 ) { %>
                                                            <p ><%= User.bio %> </p>
                                                        <% } else { %>
                                                            <p ><%= User.bio.substring(0,250) %>...</p>
                                                        <% } %>
                                                    </p>
                                                    
                                                    <div class="d-flex flex-row justify-content-center">
                                                        <% if (User.insta_url) { %>
                                                            <div class="px-4">
                                                                <a href="<%= User.insta_url %>">
                                                                    <i class="fab fa-instagram"></i>
                                                                </a>
                                                            </div>
                                                        <% } %>
                                                        <% if (User.facebook_url) { %>
                                                            <div     class="px-4">
                                                                <a href="<%= User.facebook_url %>">
                                                                    <i class="fab fa-facebook"></i>
                                                                </a>
                                                            </div>
                                                        <% } %>
                                                        <% if (User.twitter_url) { %>
                                                            <div class="px-4">
                                                                <a href="<%= User.twitter_url %>">
                                                                    <i class="fab fa-twitter"></i>
                                                                </a>
                                                            </div>
                                                        <% } %>
                                                    </div>
                                                </div>
                                            </div>
                                        </a>
                                    </div>
                                <% }); %>
                            </div>
                        </div>
                    </section>
                    
                </div>
                <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
                    <!-- Liked content -->
                    <section id="team">
                        <div class="container py-1 my-3 text-center">
                            <div class="row mb-5">
                                <div class="col my-5">
                                    <h4 class="mb-5 pb-5 display-5">Users that you have Liked</h4>
                                </div>
                            </div>     
                            <div class="row mt-2">
                                <% user.likedUser.forEach(function(User){ %>
                                    <div class="col-lg-4 col-md-6">
                                        <a href="/profile/<%= User._id %>">
                                            <div class="card">
                                                <div class="card-body">
                                                    <img src="<%= User.image %>"
                                                        alt="" class="img-fluid rounded-circle mb-3" >
                                                    <h3 class="card-title"><%= User.fullName %> </h3>
                                                    <h5 class="py-1 "><%= User.age %> &bull;  <%= User.username %> &bull;  <%= User.city %></h5>
                                                    <p>
                                                        <% if (User.bio.length <= 150 ) { %>
                                                            <p ><%= User.bio %> </p>
                                                        <% } else { %>
                                                            <p ><%= User.bio.substring(0,250) %>...</p>
                                                        <% } %>
                                                    </p>
                                                    
                                                    <div class="d-flex flex-row justify-content-center">
                                                        <% if (User.insta_url) { %>
                                                            <div class="px-4">
                                                                <a href="<%= User.insta_url %>">
                                                                    <i class="fab fa-instagram"></i>
                                                                </a>
                                                            </div>
                                                        <% } %>
                                                        <% if (User.facebook_url) { %>
                                                            <div     class="px-4">
                                                                <a href="<%= User.facebook_url %>">
                                                                    <i class="fab fa-facebook"></i>
                                                                </a>
                                                            </div>
                                                        <% } %>
                                                        <% if (User.twitter_url) { %>
                                                            <div class="px-4">
                                                                <a href="<%= User.twitter_url %>">
                                                                    <i class="fab fa-twitter"></i>
                                                                </a>
                                                            </div>
                                                        <% } %>
                                                    </div>
                                                </div>
                                            </div>
                                        </a>
                                    </div>
                                <% }); %>
                            </div>
                        </div>
                    </section>
                </div>
            </div>       
        </div>
    </div>


<script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"
    integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
